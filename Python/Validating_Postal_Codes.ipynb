        "import sys\n",
        "\n",
        "stdin = sys.stdin\n",
        "\n",
        "import re\n",
        "line = stdin.readline().strip()\n",
        "print(re.match(\"^[1-9][0-9]{5}$\", line) != None and [ord(line[i]) - ord(line[i+2]) for i in range(4)].count(0) <= 1)"
      ],
